{
  "name": "@eggjs/mock",
  "version": "6.0.0-beta.2",
  "publishConfig": {
    "access": "public"
  },
  "eggPlugin": {
    "name": "egg-mock",
    "exports": {
      "import": "./dist/esm",
      "require": "./dist/commonjs"
    }
  },
  "description": "mock server plugin for egg",
  "homepage": "https://github.com/eggjs/mock",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/mock.git"
  },
  "bugs": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "keywords": [
    "egg",
    "mock"
  ],
  "author": "popomore <sakura9515@gmail.com>",
  "engines": {
    "node": ">= 18.19.0"
  },
  "dependencies": {
    "@eggjs/core": "^6.2.4",
    "@eggjs/supertest": "^8.1.0",
    "@eggjs/utils": "^4.0.3",
    "coffee": "^5.2.1",
    "detect-port": "^2.1.0",
    "egg-logger": "^3.5.0",
    "extend2": "^4.0.0",
    "get-ready": "^3.4.0",
    "globby": "^11.1.0",
    "is-type-of": "^2.2.0",
    "merge-descriptors": "^2.0.0",
    "mm": "^4.0.1",
    "sdk-base": "^5.0.0",
    "utility": "^2.3.0"
  },
  "peerDependencies": {
    "mocha": "^10.2.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.1",
    "@eggjs/tegg": "^3.2.2",
    "@eggjs/tegg-config": "^3.2.2",
    "@eggjs/tegg-controller-plugin": "^3.2.2",
    "@eggjs/tegg-plugin": "^3.2.2",
    "@eggjs/tsconfig": "1",
    "@types/methods": "^1.1.4",
    "@types/mocha": "10",
    "@types/node": "22",
    "egg": "beta",
    "egg-bin": "beta",
    "egg-errors": "^2.2.1",
    "egg-tracer": "^2.0.0",
    "eslint": "8",
    "eslint-config-egg": "14",
    "mocha": "10",
    "tshy": "3",
    "tshy-after": "1",
    "typescript": "5",
    "urllib": "4"
  },
  "scripts": {
    "lint": "eslint --cache src test --ext .ts",
    "pretest": "npm run lint -- --fix && npm run prepublishOnly",
    "test": "egg-bin test",
    "preci": "npm run lint && npm run prepublishOnly && attw --pack",
    "ci": "egg-bin cov",
    "prepublishOnly": "tshy && tshy-after"
  },
  "type": "module",
  "tshy": {
    "exports": {
      ".": "./src/index.ts",
      "./bootstrap": "./src/bootstrap.ts",
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./bootstrap": {
      "import": {
        "types": "./dist/esm/bootstrap.d.ts",
        "default": "./dist/esm/bootstrap.js"
      },
      "require": {
        "types": "./dist/commonjs/bootstrap.d.ts",
        "default": "./dist/commonjs/bootstrap.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "types": "./dist/commonjs/index.d.ts",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js"
}
